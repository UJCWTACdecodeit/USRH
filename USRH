    public static String[] storage(int cha)//5
    {
        Scanner obj = new Scanner(System.in);
        String sto [] = new String [2];
        sto[0] = "user_1";
        sto[1] = "10100307";
        if(cha == 1)
        {
            int i;
            for (i = 0; i <2; i++)         
