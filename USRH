    public static int check_val (String answer)//3
    {
        String signin = "signin";
        String signup = "signup";
        int out = 0;
        if(answer.equals(signin))
        {
            out = 1;
        }
        else
        {
            out = 0;
        }
        return out;
    }
    public static int eve (int give_con1)//4
