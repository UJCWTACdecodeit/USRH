    }
    public static int check()
    {
        int cha = 0;
        int out = 0 ;
        String check_ans [] = new String[2];
        check_ans = usrh.storage(cha);
        Scanner obj = new Scanner(System.in);
        int i;
        String input_ans[] = new String[3];
        System.out.println("user_1,10100307");
        for (i = 0; i <2; i++)         
            {
                input_ans[i] = obj.nextLine();
